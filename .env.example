# Google Cloud Configuration
GOOGLE_APPLICATION_CREDENTIALS=/path/to/your/service-account-key.json
PROJECT_ID=your-gcp-project-id
REGION=us-central1

# PubSub Configuration
PUBSUB_SUBSCRIPTION_CLICKSTREAM=projects/${PROJECT_ID}/subscriptions/your-clickstream-sub
PUBSUB_SUBSCRIPTION_INVENTORY=projects/${PROJECT_ID}/subscriptions/your-inventory-sub
PUBSUB_SUBSCRIPTION_TRANSACTIONS=projects/${PROJECT_ID}/subscriptions/your-transactions-sub

# Redis Configuration
REDIS_HOST=127.0.0.1
REDIS_PORT=6379

# Input Files (Batch Mode)
CLICKSTREAM_BATCH_FILE=/path/to/clickstream.json
STOCK_BATCH_FILE=/path/to/stock.json
TRANSACTION_BATCH_FILE=/path/to/transactions.json
